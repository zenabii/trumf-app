<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.trumfregistrering" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Trumf Registrering</name>
    <description>
        App for registrering av Trumf-medlemmer.
    </description>
    <author email="maria.eilertsen@involve.no" href="https://involve.no">
        Maria Eilertsen, Involve AS.
    </author>
    <icon src="www/img/logo.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="fullscreen" value="true" />
</widget>
